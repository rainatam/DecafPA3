class A {
 int a;
 int a1;
 void setA(int i, int j){
  a = i;
  a1 = j;
 }
 void print(){
  Print(" a=",a," a1=",a1," "); 
 }
 void allprint()
 {
  print();
 }
 void fun(){
  Print("A");
  print();
  Print("\n");
 }
}

class B extends A{
 int b;
 int b1;
 void setB(int i, int j){
  b = i;
  b1 = j;
 }
 void print(){
  Print(" b=",b," b1=",b1," "); 
 }
 void allprint()
 {
  super.allprint();
  print();
 }
 void fun(){
  Print("B");
  super.allprint();
  print();
  Print("\n");
 }
}
