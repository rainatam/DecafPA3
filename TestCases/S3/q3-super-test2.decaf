class A {
 int a;
 int a1;
 void setA(int i, int j){
  a = i;
  a1 = j;
 }
 void print(){
  Print(" a=",a," a1=",a1," "); 
 }
 void fun(){
  Print("A");
  print();
  Print("\n");
 }
}

class B extends A{
 int b;
 int b1;
 void setB(int i, int j){
  b = i;
  b1 = j;
 }
 void print(){
  Print(" b=",b," b1=",b1," "); 
 }
 void fun(){
  Print("B");
  super.print();
  print();
  Print("\n");
 }
}


class Main {

    static void main() {
      class A a;
      class B b;
      
      a = new A();
      b = new B();

      a.setA(10,11);
      b.setA(20,21);

      a.fun();
      b.fun();

    }
}